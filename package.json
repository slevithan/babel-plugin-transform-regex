{
  "name": "babel-plugin-transform-regex",
  "version": "4.0.0",
  "description": "Babel plugin for the regex package",
  "author": "Steven Levithan",
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.js",
  "scripts": {
    "build:optimizer": "esbuild --format=esm --bundle node_modules/regexp-tree/dist/optimizer/index.js --outfile=tools/optimizer.js",
    "build:standalone": "esbuild src/index.js --bundle --minify --outfile=dist/standalone.min.js --global-name=BabelPluginTransformRegexStandalone",
    "prebuild": "rimraf --glob dist/* tools/*",
    "build": "npm run build:optimizer && npm run build:standalone",
    "pretest": "npm run build",
    "test": "jasmine",
    "prepare": "npm test"
  },
  "files": [
    "src",
    "dist",
    "tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slevithan/babel-plugin-transform-regex.git"
  },
  "keywords": [
    "regex",
    "babel-plugin"
  ],
  "dependencies": {
    "regex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "esbuild": "^0.23.0",
    "jasmine": "^5.2.0",
    "regexp-tree": "^0.1.27",
    "rimraf": "^6.0.1"
  }
}
